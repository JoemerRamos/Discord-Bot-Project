{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "no-restricted-syntax": 0,
        "global-require": 0,
        "import/no-dynamic-require": 0,
        "no-console": 0,
        "indent": 0
    }
}